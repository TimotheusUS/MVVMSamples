                    </GeometryDrawing.Pen>
                </GeometryDrawing>
                <GeometryDrawing Geometry="M 50,20 L 150,80">
                    <GeometryDrawing.Pen>
                        <Pen Brush="Black" Thickness="4" />
                    </GeometryDrawing.Pen>
                </GeometryDrawing>

                <!-- Verbindungslinien links -->
                <GeometryDrawing Geometry="M 50,20 L 50,0">
                    <GeometryDrawing.Pen>
                        <Pen Brush="Black" Thickness="4" />
                    </GeometryDrawing.Pen>
                </GeometryDrawing>

                <GeometryDrawing Geometry="M 50,80 L 50,100">
                    <GeometryDrawing.Pen>
                        <Pen Brush="Black" Thickness="4" />
                    </GeometryDrawing.Pen>
